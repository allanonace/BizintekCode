  <ItemGroup>
    <Compile Update="view\BleGattServicePage.xaml.cs">
      <DependentUpon>AclaraViewReadMTU.xaml</DependentUpon>
    </Compile>
    <Compile Update="view\BleSettingsPage.xaml.cs">
